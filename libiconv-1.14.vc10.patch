+++ libiconv-1.14.vc10/lib/iconv.c	2011-12-16 13:55:43.911183600 +0100
@@ -424,7 +424,7 @@
         namesbuf[i++] = aliasbuf[j++].name;
       while (j < num_aliases && aliasbuf[j].encoding_index == ei);
